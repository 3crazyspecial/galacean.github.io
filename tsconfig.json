{
  "compilerOptions": {
    "strictNullChecks": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "target": "es6",
    "lib": ["dom", "es2017"],
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "lib", "es"]
}
